<!doctype html>
<html>
<head>
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1">
<title>TheNeeds 5 Responsive Templates</title>
<!--[if lt IE 9]>
<script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
<script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
<![endif]-->
<!--CUSTOM CSS-->
<link href="css/custom.css" rel="stylesheet" type="text/css">
<!--BOOTSTRAP CSS-->
<link href="css/bootstrap.css" rel="stylesheet" type="text/css">
<!--COLOR CSS-->
<link href="css/color.css" rel="stylesheet" type="text/css">
<!--RESPONSIVE CSS-->
<link href="css/responsive.css" rel="stylesheet" type="text/css">
<!--OWL CAROUSEL CSS-->
<link href="css/owl.carousel.css" rel="stylesheet" type="text/css">
<!--Fontawesome CSS-->
<link href="css/font-awesome.min.css" rel="stylesheet" type="text/css">
<!--Annimation CSS-->
<link href="css/animate.css" rel="stylesheet" type="text/css">
<!--SVG CSS-->
<link href="css/svg.css" rel="stylesheet" type="text/css">
<!--Bxslider CSS-->
<link href="css/jquery.bxslider.css" rel="stylesheet" type="text/css">
<!--FAVICON ICON-->
<link rel="shortcut icon" type="image/x-icon" href="images/favicon.ico">
</head>

<body>
<!--WRAPPER START-->
<div id="wrapper"> 
  <!--HEADER START-->
 <%- include('header') %>
  <!--HEADER END--> 
  
  <!--INNER BANNER START-->
  <div id="inner-banner">
    <div class="container">
      <h1>Priority Causes</h1>
      <em>It is a long established fact that a reader will be distracted by the content</em>
      <ol class="breadcrumb">
        <li><a href="#">Home</a></li>
        <li><a href="#">Causes</a></li>
        <li class="active">Causes Modern</li>
      </ol>
    </div>
  </div>
  <!--INNER BANNER END--> 
  
  <!--MAIN START-->
  <div id="main" style="margin-top:160px;"> 
    <!--CAUSES STYLE 1 START-->
    <section class="causes-style-1 causes-grid">
      <div class="container">
        <div class="row">
      
          <% for(var i=0; i< projects.length; i++) { %>
              <div class="col-md-4">
                <div class="box">
                      <div class="frame"><a href="#"><img src="<%= projects[i].img %>" alt="img"></a></div>
                      <div class="text-box">
                        <h3><%= projects[i].title %></h3>
                        <div class="btn-row">
                        <a href="#" class="link"><i class="fa fa-user" aria-hidden="true"></i><%= projects[i].owner %></a>
                          <a href="#" class="link"><i class="fa fa-clock-o" aria-hidden="true"></i><%= projects[i].date %></a>
                          <a href="#" class="link"><i class="fa fa-map-marker" aria-hidden="true"></i><%= projects[i].address %></a> </div>
                        <div class="causes-goal-box">
                          <ul>
                            <li> <span class="title">Goal:</span> <strong class="amount"><sup>QAR</sup> <%= projects[i].goal %>  </strong> </li>
                            <li> <span class="title">Raised:</span> <strong class="amount"><sup>QAR</sup> <%= projects[i].raised %> </strong> </li>
                            <li> <span class="title">Donators:</span> <strong class="amount"> <%= projects[i].investors %> Investor</strong> </li>
                            <li> <span class="title">Time Remain:</span> <strong class="amount"> <%= projects[i].remainingDays %> Days Left</strong> </li>
                          </ul>
                          <div class="pie-title-center demo-pie-1" data-percent="<%= projects[i].percent %>"> <span class="pie-value"></span><b>completed</b> </div>
                        </div>
                        <a href="#" class="btn-style-1">Invest Now<i class="fa fa-long-arrow-right" aria-hidden="true"></i></a> </div>
                    </div>
                </div>
           <% } %>

            
          
          
        </div>
        <div class="pagination-box">
          <nav aria-label="Page navigation">
            <ul class="pagination">
              <li> <a href="#" aria-label="Previous"> <span aria-hidden="true">Prev</span> </a> </li>
              <li><a href="#">1</a></li>
              <li class="active"><a href="#">2</a></li>
              <li><a href="#">3</a></li>
              <li><a href="#">4</a></li>
              <li><a href="#">5</a></li>
              <li> <a href="#" aria-label="Next"> <span aria-hidden="true">Next</span> </a> </li>
            </ul>
          </nav>
        </div>
      </div>
    </section>
    <!--CAUSES STYLE 1 END--> 
  </div>
  <!--MAIN END--> 
  
  <!--FOOTER START-->
 <%- include('footer'); %>
  <!--FOOTER END--> 
</div>
<!--WRAPPER END--> 

<!--jQuery min--> 
<script src="js/jquery.js"></script> 
<!--BOOTSTRAP JS--> 
<script src="js/bootstrap.min.js"></script> 
<!--OWL CAROUSEL JS--> 
<script src="js/owl.carousel.min.js"></script> 
<!--progress JS--> 
<script src="js/pie-chart.js"></script> 
<!--EVENT TIMER--> 
<script src="js/jquery.plugin.js"></script> 
<script src="js/jquery.countdown.js"></script> 
<!--Bxslider JS--> 
<script src="js/jquery.bxslider.min.js"></script> 
<!--Filter Js--> 
<script src="js/sorting.js"></script> 
<script src="js/jquery.isotope.js"></script> 
<!--CUSTOM js--> 
<script src="js/custom.js"></script>
</body>
</html>
